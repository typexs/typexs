{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo-app": {
      "root": "src",
      "sourceRoot": "src/demo-app",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/demo-app/index.html",
            "main": "src/demo-app/main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "src/demo-app/polyfills.ts",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "class-validator"
            ],
            "assets": [
              "src/demo-app/assets",
              "src/demo-app/favicon.ico"
            ],
            "styles": [
              "src/demo-app/styles/styles.scss",
              "src/demo-app/styles/app.loading.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/demo-app/environments/environment.ts",
                  "with": "src/demo-app/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": true,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo-app:build:production",
              "proxyConfig": "proxy.conf.json"
            },
            "development": {
              "browserTarget": "demo-app:build:development",
              "proxyConfig": "proxy.conf.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/demo-app/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/demo-app/polyfills.ts",
            "tsConfig": "./tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/demo-app/styles/styles.scss",
              "src/demo-app/styles/app.loading.scss"
            ],
            "assets": [
              "src/demo-app/assets",
              "src/demo-app/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "demo-app",
  "schematics": {
    "@schematics/angular:class": {
      "spec": false
    },
    "@schematics/angular:component": {
      "spec": false,
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "spec": false
    },
    "@schematics/angular:guard": {
      "spec": false
    },
    "@schematics/angular:module": {
      "spec": false
    },
    "@schematics/angular:pipe": {
      "spec": false
    },
    "@schematics/angular:service": {
      "spec": false
    }
  }
}
