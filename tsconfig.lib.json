{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "es2020",
    "paths": {
      "@typexs/base": [
        "./packages/base/src/browser"
      ],
      "@typexs/base/*": [
        "./packages/base/src/*"
      ],
      "@typexs/auth": [
        "./packages/auth/src/browser"
      ],
      "@typexs/auth/*": [
        "./packages/auth/src/*"
      ],
      "@typexs/auth-ng": [
        "./packages/auth-ng/src/public_api"
      ],
      "@typexs/auth-ng/*": [
        "./packages/auth-ng/src/*"
      ],
      "@typexs/server": [
        "./packages/server/src/browser",
      ],
      "@typexs/server/*": [
        "./packages/server/src/*"
      ],
      "@typexs/entity": [
        "./packages/entity/src/browser",
      ],
      "@typexs/entity/*": [
        "./packages/entity/src/*",
      ],
      "@typexs/ng": [
        "./packages/ng/src/browser",
      ],
      "@typexs/ng/*": [
        "./packages/ng/src/*",
      ],
      "@typexs/roles": [
        "./packages/roles/src/browser",
      ],
      "@typexs/roles/*": [
        "./packages/roles/src/*",
      ],
      "@typexs/base-ng/*": [
        "./packages/base-ng/src/*"
      ],
      "@typexs/base-ng": [
        "./packages/base-ng/src/public_api"
      ],
      "@typexs/ng-forms/*": [
        "./packages/ng-forms/src/*"
      ],
      "@typexs/storage-ng/*": [
        "./packages/storage-ng/src/*"
      ],
      "@typexs/ng-router-menu/*": [
        "./packages/ng-router-menu/src/*"
      ],

    }
  }

}
