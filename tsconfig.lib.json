{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es2017",
    "module": "es2020",
    "lib": [
      "es2020",
      "dom"
    ],
    "paths": {
      "@typexs/base": [
        "./packages/base/src/browser"
      ],
      "@typexs/base/*": [
        "./packages/base/src/*"
      ],
      "@typexs/base-ng/*": [
        "./packages/base-ng/src/*"
      ],
      "@typexs/base-ng": [
        "./packages/base-ng/src/public_api"
      ],
      "@typexs/auth": [
        "./packages/auth/src/browser"
      ],
      "@typexs/auth/*": [
        "./packages/auth/src/*"
      ],
      "@typexs/auth-ng": [
        "./packages/auth-ng/src/public_api"
      ],
      "@typexs/auth-ng/*": [
        "./packages/auth-ng/src/*"
      ],
      "@typexs/server": [
        "./packages/server/src/browser"
      ],
      "@typexs/server/*": [
        "./packages/server/src/*"
      ],
      "@typexs/entity": [
        "./packages/entity/src/browser"
      ],
      "@typexs/entity/*": [
        "./packages/entity/src/*"
      ],
      "@typexs/forms": [
        "./packages/forms/src/browser"
      ],
      "@typexs/forms/*": [
        "./packages/forms/src/*"
      ],
      "@typexs/ng": [
        "./packages/ng/src/browser"
      ],
      "@typexs/ng/*": [
        "./packages/ng/src/*"
      ],
      "@typexs/roles": [
        "./packages/roles/src/browser"
      ],
      "@typexs/roles/*": [
        "./packages/roles/src/*"
      ],
      "@typexs/roles-api": [
        "./packages/roles-api/src/browser"
      ],
      "@typexs/roles-api/*": [
        "./packages/roles-api/src/*"
      ],
      "@typexs/storage": [
        "./packages/storage/src/browser"
      ],
      "@typexs/storage/*": [
        "./packages/storage/src/*"
      ],
      "@typexs/distributed-storage": [
        "./packages/distributed-storage/src/browser"
      ],
      "@typexs/distributed-storage/*": [
        "./packages/distributed-storage/src/*"
      ],
      "@typexs/search": [
        "./packages/search/src/browser"
      ],
      "@typexs/search/*": [
        "./packages/search/src/*"
      ],
      "@typexs/forms-ng/*": [
        "./packages/forms-ng/src/*"
      ],
      "@typexs/storage-ng/*": [
        "./packages/storage-ng/src/*"
      ],
      "@typexs/ng-router-menu/*": [
        "./packages/ng-router-menu/src/*"
      ]
    }
  }
}
