{
  "name": "@typexs/typexs",
  "version": "3.0.2",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "start": "npx concurrently \"cd packages/app && npx ng serve\" \"npx typexs server --nodeId server\"",
    "clean": "npx lerna clean -y && npx lerna run clean && rm node_modules -R",
    "clean:modules": "npx lerna run clean:modules && rm node_modules -R",
    "clean:build": "npx lerna run clean:build",
    "test": "npx lerna run test --concurrency=1",
    "__build": "npm run clean:build && npx lerna run build --concurrency=1",
    "build": "npm run clean:build && for i in `npx lerna list --toposort`; do npx lerna run build --scope=$i; done",
    "job:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && docker run --volume $PWD:/data -it pandoc/core -t asciidoc -f markdown CHANGELOG.md > docs/modules/ROOT/pages/changelog.adoc && git add CHANGELOG.md docs/modules/ROOT/pages/changelog.adoc",
    "packages:publish": "npx lerna publish from-package",
    "packages:version": "./scripts/version.sh",
    "documentation": "npx antora --stacktrace generate --clean --fetch --log-level all --to-dir docs/build/site",
    "documentation:watch": "npx nodemon --watch docs -e adoc --exec npm run documentation -- docs/antora-playbook.yml",
    "version": "if [ -f .version-pass ]; then npm run job:changelog; fi"
  },
  "workspaces": [
    "packages/*/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "angular",
    "framework",
    "commons",
    "typescript",
    "typexs"
  ],
  "devDependencies": {
