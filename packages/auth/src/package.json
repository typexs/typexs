{
  "name": "@typexs/auth",
  "version": "3.0.3",
  "description": "Auth module for typexs",
  "main": "index",
  "browser": "browser",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:modules",
    "clean:build": "node ../../../scripts/clean.js build",
    "clean:modules": "node ../../../scripts/clean.js modules",
    "test": "nyc mocha ../test/{**,**/**,**/**/**}/*.spec.ts",
    "posttest": "nyc report --reporter=json",
    "build": "node ../../../scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "typexs",
    "typescript",
    "auth"
  ],
  "dependencies": {
    "@typexs/base": "^3.0.3",
    "@typexs/entity": "^3.0.3",
    "@typexs/forms": "^3.0.3",
    "@typexs/roles": "^3.0.3",
    "@typexs/roles-api": "^3.0.1",
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@typexs/server": "^3.0.3",
    "@typexs/testing": "^3.0.3"
  },
  "peerDependencies": {
    "@typexs/server": ">=3.0.0"
  },
  "typexs": {
    "name": "auth",
    "declareLibs": [
      {
        "topic": "auth.adapters",
        "refs": [
          "adapters/auth/*/*Adapter.*",
          "src/adapters/auth/*/*Adapter.*"
        ]
      },
      {
        "topic": "auth.configurations",
        "refs": [
          "adapters/auth/*/*Configuration.*",
          "src/adapters/auth/*/*Configuration.*"
        ]
      }
    ]
  },
  "publishConfig": {
    "tag": "v3-lts"
  },
  "gitHead": "a845caa5b33bf3e91c285565671b774d1f61fa4a"
}
