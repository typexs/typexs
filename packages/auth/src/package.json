{
  "name": "@typexs/auth",
  "version": "2.3.4",
  "description": "Auth module for typexs",
  "main": "index",
  "browser": "browser",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:modules",
    "clean:build": "rm -rf ../build",
    "clean:modules": "rm -rf node_modules ../node_modules",
    "test": "nyc mocha ../test/{**,**/**,**/**/**}/*.spec.ts",
    "posttest": "nyc report --reporter=json",
    "build": "node ../../../scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "typexs",
    "typescript",
    "auth"
  ],
  "dependencies": {
    "@typexs/base": "^2.3.4",
    "@typexs/entity": "^2.3.4",
    "@typexs/forms": "^2.3.4",
    "@typexs/roles": "^2.3.4",
    "@typexs/roles-api": "^2.3.1",
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@typexs/server": "^2.3.4",
    "@typexs/testing": "^2.3.4"
  },
  "peerDependencies": {
    "@typexs/server": ">=2.3.0"
  },
  "typexs": {
    "name": "auth",
    "declareLibs": [
      {
        "topic": "auth.adapters",
        "refs": [
          "adapters/auth/*/*Adapter.*",
          "src/adapters/auth/*/*Adapter.*"
        ]
      },
      {
        "topic": "auth.configurations",
        "refs": [
          "adapters/auth/*/*Configuration.*",
          "src/adapters/auth/*/*Configuration.*"
        ]
      }
    ]
  },
  "publishConfig": {
    "tag": "v2.3"
  },
  "gitHead": "00d1140a17b92fc98d1a7914810478380c16ee35"
}
