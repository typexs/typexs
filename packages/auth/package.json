{
  "name": "@typexs/auth",
  "version": "2.2.0",
  "description": "Auth module for typexs",
  "main": "index",
  "browser": "browser",
  "scripts": {
    "test": "nyc mocha test/{**,**/**,**/**/**}/*.spec.ts",
    "posttest": "nyc report --reporter=json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "typexs",
    "typescript",
    "auth"
  ],
  "dependencies": {
    "@typexs/base": "*",
    "@typexs/entity": "*",
    "@typexs/roles": "*",
    "@typexs/ng": "*",
    "@typexs/roles-api": "*",
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "peerDependencies": {
    "@typexs/server": "*"
  },
  "private": true,
  "typexs": {
    "name": "auth",
    "declareLibs": [
      {
        "topic": "auth.adapters",
        "refs": [
          "adapters/auth/*/*Adapter.*",
          "src/adapters/auth/*/*Adapter.*"
        ]
      },
      {
        "topic": "auth.configurations",
        "refs": [
          "adapters/auth/*/*Configuration.*",
          "src/adapters/auth/*/*Configuration.*"
        ]
      }
    ]
  }
}
