{
  "name": "@typexs/server",
  "version": "0.0.0-PLACEHOLDER",
  "description": "Servers for typexs",
  "main": "./index.ts",
  "browser": "./browser.ts",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha test/{**,**/**,**/**/**}/*.spec.ts",
    "posttest": "nyc report --reporter=json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript",
    "typexs",
    "server",
    "controllers"
  ],
  "bugs": {
    "url": "https://gitlab.com/typexs/typexs/-/issues"
  },
  "homepage": "https://gitlab.com/typexs/typexs#readme",
  "dependencies": {
    "@typexs/roles-api": "0.0.0-PLACEHOLDER",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "routing-controllers": "0.9.0"
  },
  "peerDependencies": {
    "@typexs/base": "0.0.0-PLACEHOLDER"
  },
  "private": true,
  "typexs": {
    "name": "server",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "controllers",
          "src/controllers",
          "src/modules/*/controllers"
        ]
      },
      {
        "topic": "server.middleware",
        "refs": [
          "middleware",
          "src/middleware"
        ]
      }
    ]
  }
}
