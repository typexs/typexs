{
  "name": "@typexs/server",
  "version": "3.0.0",
  "description": "Servers for typexs",
  "main": "index",
  "browser": "browser",
  "directories": {
    "lib": "",
    "test": "../test"
  },
  "scripts": {
    "clean": "npm run clean:build && npm run clean:modules",
    "clean:build": "rm -rf ../build",
    "clean:modules": "rm -rf node_modules ../node_modules",
    "test": "nyc mocha ../test/{**,**/**,**/**/**}/*.spec.ts",
    "posttest": "nyc report --reporter=json",
    "build": "node ../../../scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript",
    "typexs",
    "server",
    "controllers"
  ],
  "bugs": {
    "url": "https://gitlab.com/typexs/typexs/-/issues"
  },
  "homepage": "https://gitlab.com/typexs/typexs#readme",
  "dependencies": {
    "@typexs/base": "^3.0.0",
    "@typexs/roles-api": "^3.0.0"
  },
  "devDependencies": {
    "@typexs/testing": "^3.0.0"
  },
  "peerDependencies": {
    "@typexs/base": ">=3.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "routing-controllers": "0.10.4"
  },
  "typexs": {
    "name": "server",
    "declareLibs": [
      {
        "topic": "server.controllers",
        "refs": [
          "controllers",
          "src/controllers",
          "src/modules/*/controllers"
        ]
      },
      {
        "topic": "server.middleware",
        "refs": [
          "middleware",
          "src/middleware"
        ]
      }
    ]
  },
  "publishConfig": {
    "tag": "v3-lts"
  },
  "gitHead": "00d1140a17b92fc98d1a7914810478380c16ee35"
}
