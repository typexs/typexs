{
  "name": "@typexs/storage",
  "version": "3.0.1",
  "description": "Storage for typexs",
  "main": "index",
  "browser": "browser",
  "directories": {
    "lib": "",
    "test": "../test"
  },
  "scripts": {
    "clean": "npm run clean:build && npm run clean:modules",
    "clean:build": "rm -rf ../build",
    "clean:modules": "rm -rf node_modules ../node_modules",
    "test": "nyc mocha ../test/{**,**/**,**/**/**}/*.spec.ts",
    "posttest": "nyc report --reporter=json",
    "build": "node ../../../scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript",
    "typexs"
  ],
  "bugs": {
    "url": "https://gitlab.com/typexs/typexs/-/issues"
  },
  "homepage": "https://gitlab.com/typexs/typexs#readme",
  "dependencies": {
    "@typexs/roles-api": "^3.0.1"
  },
  "devDependencies": {
    "@typexs/base": "^3.0.1",
    "@typexs/forms": "^3.0.1",
    "@typexs/server": "^3.0.1",
    "@typexs/testing": "^3.0.1"
  },
  "peerDependencies": {
    "@typexs/base": ">=3.0.0",
    "@typexs/forms": ">=3.0.0",
    "@typexs/server": ">=3.0.0"
  },
  "typexs": {
    "name": "storage",
    "declareLibs": [
      {
        "topic": "entity.storage",
        "refs": [
          "entities/storage",
          "src/entities/storage"
        ]
      }
    ]
  },
  "publishConfig": {
    "tag": "v3-lts"
  },
