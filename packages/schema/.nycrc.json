{
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "**/*.d.ts",
    "tmp/",
    "build/"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "html",
    "json",
    "lcovonly"
  ],
  "report-dir": "coverage/schema",
  "sourceMap": true,
  "instrument": true,
  "cache": false
}
