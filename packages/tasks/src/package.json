{
  "name": "@typexs/tasks",
  "version": "3.0.0",
  "description": "Tasks for typexs",
  "main": "index",
  "browser": "browser",
  "directories": {
    "lib": "",
    "test": "../test"
  },
  "scripts": {
    "clean": "npm run clean:build && npm run clean:modules",
    "clean:build": "rm -rf ../build",
    "clean:modules": "rm -rf node_modules ../node_modules",
    "test": "nyc mocha ../test/{**,**/**,**/**/**}/*.spec.ts",
    "posttest": "nyc report --reporter=json",
    "build": "node ../../../scripts/build.js"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "typescript",
    "typexs",
    "tasks"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/typexs/typexs.git"
  },
  "bugs": {
    "url": "https://gitlab.com/typexs/typexs/-/issues"
  },
  "homepage": "https://gitlab.com/typexs/typexs#readme",
  "dependencies": {
    "@allgemein/queue": "^1.1.1"
  },
  "devDependencies": {
    "@typexs/base": "^3.0.0",
    "@typexs/testing": "^3.0.0",
    "@typexs/server": "^3.0.0",
    "@typexs/generic": "^3.0.0"
  },
  "peerDependencies": {
    "@typexs/base": ">=2.4.0",
    "@typexs/generic": ">=2.4.0",
    "@typexs/server": ">=2.4.0"
  },
  "typexs": {
    "name": "tasks",
    "declareLibs": [
      {
        "topic": "tasks",
        "refs": [
          "tasks",
          "tasks/*/*",
          "src/tasks",
          "src/tasks/*/*"
        ]
      }
    ]
  },
  "publishConfig": {
    "tag": "v3-lts"
  },
  "gitHead": "00d1140a17b92fc98d1a7914810478380c16ee35"
}
